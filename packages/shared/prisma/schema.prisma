generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model client {
  id           String   @id
  name         String
  surname      String
  email        String
  number_phone String
  birth_date   DateTime
  created_at   DateTime @default(now())
  updated_at   DateTime
}
